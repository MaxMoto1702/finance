import finance.Account
import finance.Document
import finance.DocumentRow

model {
	Document document
}

//json tmpl.document(document)
json {
    id document.id
    type document.type
    company document.company
    account document.account, { Account account ->
        id account.id
        name account.name
    }
    date document.date
    amount document.amount
    description document.description
    rows document.rows ?: [], { DocumentRow row ->
        id row.id
    }
}